@model ExamDev.Infra.Authen


<div class="container">
    <form method="post" asp-controller="Authen" asp-action="Create">
    <div class="card col-md-12 justify-content-md-center p-3">
    <div class="mb-4">
        <h3>Authen Create</h3>
    </div>
    <div class="mb-3">
        <label asp-for="AuthenName"></label>
        <input asp-for="AuthenName" type="text" value="" class="form-control "/>
        <span asp-validation-for="AuthenName" class="text-danger"></span>
    </div>
    <div class="d-flex">
        <a asp-action="Index" class="btn btn-secondary me-2">Back</a>
        <button type ="submit"class="btn btn-success">Save</button>
    </div>
    </div>
    </form>
</div>
